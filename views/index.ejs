<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <% messages.forEach(message => { %>
    <div class="message"><%= message.text %></div>
    <div class="user"><%= message.user %></div>
    <div class="date"><%=message.added %></div>
    <% }) %>
  </body>
</html>
